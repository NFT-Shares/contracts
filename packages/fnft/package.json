{
  "name": "@nft-shares/fnft",
  "version": "1.0.1",
  "description": "fnft smart contracts for interacting with nft shares Protocol",
  "license": "GPL-2.0-or-later",
  "publishConfig": {
    "access": "public"
  },
  "keywords": [
    "nft shares",
    "fnft"
  ],
  "files": [
    "contracts"
  ],
  "engines": {
    "node": ">=10"
  },
  "scripts": {
    "compile": "yarn hardhat compile",
    "deploy": "hardhat run scripts/deploy.ts --network ",
    "deploy:xrpl:testnet": "yarn hardhat deploy --tags testnet --network xrpl_devnet",
    "verify": "hardhat run scripts/verify.ts --network"
  },
  "devDependencies": {
    "@nomicfoundation/hardhat-toolbox": "^2.0.2",
    "@nomiclabs/hardhat-ethers": "^2.2.3",
    "@nomiclabs/hardhat-waffle": "^2.0.1",
    "@openzeppelin/contracts": "3.4.2-solc-0.7",
    "@openzeppelin/contracts-upgradeable": "3.4.2-solc-0.7",
    "@openzeppelin/hardhat-upgrades": "^1.22.1",
    "@typechain/ethers-v5": "^7.2.0",
    "@typechain/hardhat": "^2.3.1",
    "@uniswap/lib": "4.0.1-alpha",
    "base64-sol": "1.1.0",
    "dotenv": "^16.0.3",
    "hardhat": "^2.9.3",
    "hardhat-abi-exporter": "^2.2.0",
    "hardhat-contract-sizer": "^2.6.1",
    "hardhat-deploy": "^0.9.27",
    "hardhat-gas-reporter": "^1.0.4",
    "hardhat-interface-generator": "^0.0.6",
    "solidity-coverage": "^0.7.17",
    "typechain": "^5.2.0"
  }
}
